type Query {}

type Document {
    id: ID!
    type: String!
    contents: String!
}

type Application {
    id: ID!
    type: String!
    createTimestamp: String!
    applicant: Applicant
    documents: [Document]
}

type Applicant {
    id: ID!
    firstName: String!
    lastName: String!
    email: String!
}

extend type Query {
    getApplicationById(id: ID!) : Application
    applications: [Application]!
}
